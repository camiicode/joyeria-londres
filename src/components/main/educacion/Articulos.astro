  <script>
    async function calcularTiempoLectura(url) {
      try {
        const res = await fetch(url);
        if (!res.ok) return null;
        
        const html = await res.text();
        const temp = document.createElement('div');
        temp.innerHTML = html;
        const contenido = temp.querySelector('#wrapwrap');
        
        if (!contenido) return null;

        const texto = contenido.innerText || contenido.textContent;
        const palabras = texto.trim().split(/\s+/).filter(word => word.length > 0).length;

        // Promedio de tiempo de lectura de un adulto de palabras por minuto
        return Math.ceil(palabras / 238);
        
      } catch {
        return null;
      }
    }

    document.addEventListener('DOMContentLoaded', async () => {
      const thumbnails = document.querySelectorAll('.jl-edu-articles__thumbnail');
      
      for (const thumbnail of thumbnails) {
        const url = thumbnail.href || thumbnail.getAttribute('data-url');
        const span = thumbnail.querySelector('.jl-edu-articles__thumbnail--info-time span');
        
        if (!url || !span) continue;

        const tiempo = await calcularTiempoLectura(url);
        span.textContent = tiempo ? `${tiempo} min` : '—';
      }
    });
  </script>
